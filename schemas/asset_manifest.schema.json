{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["assets"],
  "properties": {
    "assets": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "asset_id",
          "class",
          "subtype",
          "version",
          "format",
          "purpose",
          "input_hash",
          "output_hash",
          "generator_version",
          "created_at"
        ],
        "properties": {
          "asset_id": { "type": "string" },
          "class": { "type": "string" },
          "subtype": { "type": "string" },
          "version": { "type": "string" },
          "dimensions": { "type": "string" },
          "duration": { "type": "string" },
          "format": { "type": "string" },
          "purpose": { "type": "string" },
          "input_hash": { "type": "string" },
          "output_hash": { "type": "string" },
          "generator_version": { "type": "string" },
          "created_at": { "type": "string", "format": "date-time" }
        }
      }
    }
  }
}